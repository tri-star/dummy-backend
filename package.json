{
  "name": "dummy-backend",
  "version": "1.0.0",
  "description": "dummy-backend",
  "main": "serverless.ts",
  "scripts": {
    "format": "run-s format:*",
    "format:eslint": "eslint --fix \"src/*.ts\"",
    "format:prettier": "prettier --write \"src/*.ts\"",
    "lint": "run-s lint:*",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint \"src/**/*.ts\"",
    "lint:prettier": "prettier --check \"src/**/*.ts\"",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "overrides": {
    "@hono/zod-openapi": {
      "@asteasolutions/zod-to-openapi": {
        "openapi3-ts": "4.3.1"
      }
    }
  },
  "dependencies": {
    "@hono/swagger-ui": "^0.4.1",
    "@hono/zod-openapi": "^0.16.2",
    "@supabase/supabase-js": "^2.45.4",
    "aws-xray-sdk-core": "^3.10.1",
    "hono": "^4.6.3",
    "ulid": "^2.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.145",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.4",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "dotenv": "^16.4.5",
    "esbuild": "^0.24.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.10.3",
    "eslint-plugin-promise": "^7.1.0",
    "jest": "^29.7.0",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "serverless": "^4.4.4",
    "serverless-better-credentials": "^2.0.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-esbuild": "^1.54.3",
    "serverless-offline": "^14.3.2",
    "serverless-offline-ssm": "^6.2.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
